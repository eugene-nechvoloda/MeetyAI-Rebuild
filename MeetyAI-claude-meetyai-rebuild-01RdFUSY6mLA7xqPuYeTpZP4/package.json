{
  "name": "meetyai-rebuild",
  "version": "2.0.0",
  "description": "AI-powered meeting transcript analysis with dual-model architecture",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "check": "tsc --noEmit",
    "format": "prettier --write '**/*.ts'"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.1",
    "@prisma/client": "^6.18.0",
    "@slack/bolt": "^3.19.0",
    "@slack/web-api": "^7.9.3",
    "express": "^4.18.2",
    "dotenv": "^17.2.0",
    "pino": "^9.9.4",
    "pino-pretty": "^11.0.0",
    "openai": "^4.67.0",
    "zod": "^3.25.76",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.2",
    "@types/crypto-js": "^4.2.1",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3"
  }
}
